{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "react-webpack",
  "main": "index.js",
  "sideEffects": true,
  "scripts": {
    "dev:dev": "cross-env NODE_ENV=development BASE_ENV=development webpack-dev-server -c build/webpack.dev.js",
    "dev:test": "cross-env NODE_ENV=development BASE_ENV=test webpack-dev-server -c build/webpack.dev.js",
    "dev:pre": "cross-env NODE_ENV=development BASE_ENV=pre webpack-dev-server -c build/webpack.dev.js",
    "dev:prod": "cross-env NODE_ENV=development BASE_ENV=production webpack-dev-server -c build/webpack.dev.js",
    "build:dev": "cross-env NODE_ENV=production BASE_ENV=development webpack -c build/webpack.prod.js",
    "build:test": "cross-env NODE_ENV=production BASE_ENV=test webpack -c build/webpack.prod.js",
    "build:pre": "cross-env NODE_ENV=production BASE_ENV=pre webpack -c build/webpack.prod.js",
    "build:prod": "cross-env NODE_ENV=production BASE_ENV=production webpack -c build/webpack.prod.js",
    "build:analy": "cross-env NODE_ENV=production BASE_ENV=production webpack -c build/webpack.analy.js",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\"",
    "lint:style": "stylelint \"**/*.css\"",
    "prettier:fix": "prettier --write \"src/**/*\" --end-of-line auto --ignore-unknown",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "eslint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "prepare": "husky install",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "clean:lock": "rm -rf package-lock.json yarn.lock pnpm-lock.yaml",
    "clean:dist": "rm -rf ./dist",
    "clean:modules": "rm -rf node_modules"
  },
  "keywords": [
    "React",
    "Webpack",
    "React Router",
    "ESLint",
    "TSX"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-proposal-decorators": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@sentry/webpack-plugin": "^2.14.1",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^9.1.3",
    "commitizen": "^4.3.0",
    "compression-webpack-plugin": "^11.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.36.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "glob-all": "^3.3.1",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lint-staged": "^15.2.2",
    "mini-css-extract-plugin": "^2.8.0",
    "postcss-loader": "^8.1.0",
    "prettier": "^3.2.5",
    "purgecss-webpack-plugin": "^4.1.3",
    "react-refresh": "^0.14.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.4",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-prettier": "^5.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "thread-loader": "^4.0.2",
    "webpack": "^5.90.2",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.9.1",
    "webpack-merge": "^5.10.0",
    "webpackbar": "^6.0.1"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.18.4",
    "@react-spring/web": "^9.7.3",
    "@reduxjs/toolkit": "^2.2.1",
    "@sentry/cli": "^2.28.6",
    "@sentry/react": "^7.102.0",
    "@sentry/tracing": "^7.102.0",
    "antd": "^5.14.1",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.2",
    "html2canvas": "^1.4.1",
    "markmap-lib": "^0.16.0",
    "markmap-view": "^0.16.0",
    "mermaid": "^10.8.0",
    "qrcode": "^1.5.3",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.1",
    "react-sticky": "^6.0.3",
    "react-sticky-ts": "^1.0.7",
    "react-zoom-pan-pinch": "^3.4.3",
    "redux-logger": "^3.0.6",
    "rehype-highlight": "^7.0.0",
    "rehype-katex": "^7.0.0",
    "rehype-raw": "^7.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "screenfull": "^6.0.2",
    "use-debounce": "^10.0.0",
    "video.js": "^8.10.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "post-merge": "install-changed",
      "post-commit": "exec < /dev/tty && node webpack/check-version.js"
    }
  },
  "lint-staged": {
    "src/**/*.{js, jsx, ts, tsx}": [
      "npm run eslint:fix"
    ],
    "**/*": [
      "npm run prettier:fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
